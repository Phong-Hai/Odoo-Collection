<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="view_product_tree" model="ir.ui.view">
        <field name="name">custody.tree</field>
        <field name="model">custody</field>
        <field name="arch" type="xml">
            <tree>
                <field name="custody"/>
                <field name="image"/>
                <field name="ssi"/>
                <field name="description"/>
                <field name="company"/>
                <field name="product"/>
                <field name="lsnum"/>
                <field name="quan"/>
                <field name="delivered"/>
                <field name="uom"/>
            </tree>
        </field>
    </record>

    <record id="view_custody_form" model="ir.ui.view">
        <field name="name">product.form</field>
        <field name="model">custody</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="custody"/>
                            <field name="image"/>
                            <field name="ssi"/>
                            <field name="description"/>
                            <field name="company"/>
                            <field name="product"/>
                            <field name="lsnum"/>
                            <field name="quan"/>
                            <field name="delivered"/>
                            <field name="uom"/>
                        </group>
                        <group>
                            <field name="ppn"/>
                            <field name="msi"/>
                            <field name="ppf"/>
                            <field name="sno"/>
                            <field name="source"/>
                            <field name="returned"/>
                            <field name="aqty"/>
                            <field name="job"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_product" model="ir.actions.act_window">
        <field name="name">Poduct</field>
        <field name="res_model">custody</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your Product !
            </p>
        </field>
    </record>

</odoo>